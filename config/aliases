e.=explorer .
gl=git log --oneline --all --graph --decorate  $*
ls=ls --show-control-chars -F --color $*
pwd=cd
clear=cls
history=cat %CMDER_ROOT%\config\.history
unalias=alias /d $1
gca=git commit -a -m $*  
gco=git checkout $*  
gpush=git push origin $*  
gpull=git pull origin $*  
cdgo=D: $T cd D:\Workspace\go  
